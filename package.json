{
    "name": "fwge",
    "description": "Web based game engine library, built on top of the WebGL API",
    "private": true,
    "homepage": "https://github.com/Tebogo-Seshibe/fwge#Readme.md",
    "bugs": "https://github.com/Tebogo-Seshibe/fwge/issues",
    "author": {
        "name": "Tebogo Christopher Seshibe",
        "email": "tseshibe@gmail.com"
    },
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=7.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Tebogo-Seshibe/fwge.git"
    },
    "scripts": {
        "print-2": "node scripts/print",
        "build-2": "node scripts/build",
        "init-2": "node scripts/init",
        "clean-2": "node scripts/clean",
        "link-2": "node scripts/link",
        "test": "lerna run test",
        "build": "lerna run build",
        "link": "npx link",
        "add": "npm i",
        "build:watch": "lerna watch -- lerna run build --scope=@fwge/*",
        "link:watch": "lerna watch -- lerna run link --scope=@fwge/*",
        "play:cs-go": "npm i && cd apps/examples/cs-go && npm i && npm run dev"
    },
    "license": "MIT",
    "keywords": [
        "fwge",
        "gamedev",
        "webgl",
        "typescript",
        "pwa",
        "html5",
        "game-engine",
        "game-development",
        "webgl2",
        "3d"
    ],
    "workspaces": [
        "packages/common",
        "packages/core",
        "packages/render",
        "packages/audio",
        "packages/input",
        "packages/physics",
        "packages/animation",
        "packages/ui",
        "packages/io"
    ],
    "devDependencies": {
        "lerna": "^6.4.1"
    }
}
