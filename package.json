{
    "name": "fwge",
    "description": "Web based game engine library, built on top of the WebGL API",
    "private": true,
    "homepage": "https://github.com/Tebogo-Seshibe/fwge#Readme.md",
    "bugs": "https://github.com/Tebogo-Seshibe/fwge/issues",
    "author": {
        "name": "Tebogo Christopher Seshibe",
        "email": "tseshibe@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Tebogo-Seshibe/fwge.git"
    },
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "clean": "pnpm clean:libs && pnpm clean:root",
        "clean:libs": "turbo run clean",
        "clean:root": "rimraf node_modules",
        "editor": "cd apps/editor && npm i && npm run tauri"
    },
    "license": "MIT",
    "keywords": [
        "fwge",
        "webgl",
        "webgl2",
        "gamedev",
        "typescript",
        "html5",
        "game-engine",
        "game-development",
        "3d"
    ],
    "devDependencies": {
        "@types/jsdom": "^20.0.1",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "@vitest/coverage-v8": "^0.34.1",
        "eslint": "^8.44.0",
        "jsdom": "^20.0.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^6.0.1",
        "turbo": "^2.5.2",
        "typescript": "^5.8.3",
        "vitest": "^0.34.1"
    },
    "packageManager": "pnpm@9.1.3",
    "engines": {
        "node": ">=14.0.0"
    }
}
