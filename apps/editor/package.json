{
    "name": "editor",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "css": "npx tailwindcss -i ./src/tailwind.css -o ./src/app.css --watch",
        "css:watch": "pnpm css -- --watch",
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write .",
        "tauri": "tauri"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^2.0.0",
        "@sveltejs/adapter-static": "^1.0.0-next.50",
        "@sveltejs/kit": "^1.20.4",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-svelte": "^2.30.0",
        "flowbite": "^2.3.0",
        "flowbite-svelte": "^0.46.1",
        "flowbite-svelte-icons": "^1.6.1",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.10.1",
        "svelte": "^4.0.0",
        "svelte-check": "^3.4.3",
        "tslib": "^2.4.1",
        "typescript": "^5.0.0",
        "vite": "^4.3.6"
    },
    "type": "module",
    "packageManager": "pnpm@9.1.3",
    "dependencies": {
        "@fwge/audio": "workspace:*",
        "@fwge/cli": "workspace:*",
        "@fwge/common": "workspace:*",
        "@fwge/core": "workspace:*",
        "@fwge/ecs": "workspace:*",
        "@fwge/input": "workspace:*",
        "@fwge/physics": "workspace:*",
        "@fwge/render": "workspace:*",
        "@fwge/ui": "workspace:*",
        "@tauri-apps/api": "^1.4.0",
        "reflect-metadata": "^0.1.13",
        "svelte-add": "^2.0.10",
        "svelte-material-icons": "^3.0.5",
        "tailwindcss": "^3.4.3"
    }
}