        "esbuild": "~0.14.54",
        "reflect-metadata": "^0.1.14",
        "rollup-plugin-swc": "^0.2.1",
        "svelte-add": "^2.1.0",
        "svelte-material-icons": "^3.0.5",
        "tailwindcss": "^3.4.4"
