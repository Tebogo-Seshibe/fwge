{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "link",
          "test"
        ]
      }
    }
  },
  "targetDefaults": {
    "add": {
      "dependsOn": [
        "^add"
      ]
    },
    "build": {
      "dependsOn": [
        "^build",
        "^add"
      ],
      "outputs": [
        "{projectRoot}/lib"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": [
        "{projectRoot}/coverage"
      ]
    },
    "link": {
      "dependsOn": [
        "^build",
        "^link"
      ]
    }
  }
}
